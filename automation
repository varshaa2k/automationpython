from selenium  import webdriver
driver = webdriver.Chrome()
driver.get("https://www.youtube.com/")
mailbutton=driver.find_element_by_xpath("/html/body/ytd-app/div/div/ytd-masthead/div[3]/div[3]/div[2]/ytd-button-renderer/a/paper-button")
mailbutton.click()
mail=driver.find_element_by_xpath("/html/body/div[1]/div[1]/div[2]/div/div[2]/div/div/div[2]/div/div[1]/div/form/span/section/div/div/div[1]/div/div[1]/div/div[1]/input")
mail.send_keys("your mail id")
mailbutton=driver.find_element_by_xpath("/html/body/div[1]/div[1]/div[2]/div/div[2]/div/div/div[2]/div/div[2]/div/div[1]/div/div/button")
mailbutton.click()
driver.implicitly_wait(4)
driver.find_element_by_name("password").send_keys("your password")
driver.find_element_by_css_selector('#passwordNext > div > button').click()
inputElement = driver.find_element_by_name("search_query")
inputElement.send_keys("https://www.youtube.com/channel/UCO6qZqnuywSIXjdQwnH6sgg")
inputElement.submit()
driver.find_element_by_css_selector('#subscribe-button > ytd-subscribe-button-renderer > paper-button').click()
driver.find_element_by_css_selector('#main-link').click()
tikka=driver.find_element_by_xpath("/html/body/ytd-app/div/ytd-page-manager/ytd-browse/ytd-two-column-browse-results-renderer/div[1]/ytd-section-list-renderer/div[2]/ytd-item-section-renderer/div[3]/ytd-shelf-renderer/div[1]/div[2]/yt-horizontal-list-renderer/div[2]/div/ytd-grid-video-renderer[1]/div[1]/div[1]/div[1]/h3/a")
tikka.click()
driver.find_element_by_css_selector('#-level-buttons > ytd-toggle-button-renderer:nth-child(1) > a').click()
